﻿@{
    ViewBag.Title = "Home Page";
}

@{
    string name = "UmeHani";
    string companyName = "Connect Hani";
    int number = 123456;
    var num = new int[] { 1, 2, 3, 4, 5, 6, 7 };
}


<h1>@ViewBag.data</h1>
<h1>@ViewBag.Title</h1>

<h1>Hello @name!</h1>
<p>Company Name: @companyName</p>
<p>Number: @number</p>

<ul>
    @foreach (var n in num)
    {
        <li>@n</li>
    }
</ul>

